{
  "name": "goat-royalty-app",
  "version": "1.0.0",
  "description": "Music royalty management platform for artists and rights holders",
  "authors": [
    "HARVEY L MILLER JR <harvey@goatroyaltyapp.com>",
    "JUAQUIN J MALPHURS <juaquin@goatroyaltyapp.com>",
    "KEVIN W HALLINGQUEST <kevin@goatroyaltyapp.com>"
  ],
  "main": "main.js",
  "license": "UNLICENSED",
  "copyright": "Copyright Â© 2024 HARVEY L MILLER JR / JUAQUIN J MALPHURS / KEVIN W HALLINGQUEST. All rights reserved.",
  "owner": "HARVEY L MILLER JR / JUAQUIN J MALPHURS / KEVIN W HALLINGQUEST",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "electron .",
    "start-electron": "npm run build &amp;&amp; electron .",
    "build:win": "electron-builder --win",
    "build:portable": "electron-builder --win portable",
    "electron:build:win": "electron-builder --win",
    "electron:build:mac": "electron-builder --mac",
    "electron:build:linux": "electron-builder --linux",
    "electron-builder": "electron-builder",
    "postinstall": "echo 'Postinstall complete'"
  },
  "build": {
    "appId": "com.goatroyalty.app",
    "productName": "GOAT Royalty App",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "start-server.js",
      ".next/**/*",
      "public/**/*",
      "pages/**/*",
      "components/**/*",
      "styles/**/*",
      "node_modules/**/*",
      "package.json",
      "next.config.js"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "portable": {
      "artifactName": "GOAT-Royalty-App-Portable.exe"
    },
    "publish": null
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sentry/nextjs": "^10.22.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "libreoffice-convert": "^1.7.0",
    "lucide-react": "^0.309.0",
    "marked": "^16.4.1",
    "next": "^14.1.0",
    "next-auth": "^4.24.13",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "autoprefixer": "^10.4.16",
    "electron": "^28.1.3",
    "electron-builder": "^26.8.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
